1) Nth root of integer


int NthRoot(int n, int m) {
  
    int ans=-1;
  long long int s=1;
   long long int e=m;

   while(s<=e){
      int mid=(s+e)/2;
     long long int x=1;
      for(int i=1;i<=n;i++){
         x=x*mid;
         if(x>m)break;
      }

      if(x==m){
         ans=mid;
         break;
      }
      else if(x>m)e=mid-1;
      else s=mid+1;
      
     

   }
   return ans;

}

